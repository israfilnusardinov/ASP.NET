        <div class="col-lg-10">
            <!--begin::Advance Table Widget 4-->
            <div class="card card-custom card-stretch gutter-b">
                <!--begin::Header-->
                <div class="card-header border-0 py-5">
                    <h3 class="card-title align-items-start flex-column">
                        <span class="card-label font-weight-bolder text-dark">Menüler</span>
                        <span class="text-muted mt-3 font-weight-bold font-size-sm">Sitenizde ekli Menü Sayısı (@Model.Count)</span>
                    </h3>
                    <div class="card-toolbar">
                        <a href="~/Admin/Menu/Detay" class="btn btn-info font-weight-bolder font-size-sm mr-3">Yeni Menü</a>
                    </div>
                </div>
                <!--end::Header-->
                <!--begin::Body-->
                <div class="card-body pt-0 pb-3">
                    <div class="tab-content">
                        <!--begin::Table-->
                        <div class="table-responsive">
                            <table class="table table-head-custom table-head-bg table-borderless table-vertical-center">
                                <thead>
                                    <tr class="text-left text-uppercase">
                                        <th style="min-width: 250px" class="pl-7">
                                            <span class="text-dark-75">Adı</span>
                                        </th>
                                        <th style="min-width: 100px">Durum</th>
                                        <th style="min-width: 80px">Düzenle</th>
                                        <th style="min-width: 80px">SEOLink</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                <span class="text-dark-75 font-weight-bolder d-block font-size-lg">@item.Baslik</span>
                                            </td>
                                            @if (item.Durum == 1)
                                            {
                                                <td>
                                                    <span class="text-dark-75 font-weight-bolder d-block font-size-lg">Aktif</span>
                                                </td>
                                            }
                                            else
                                            {
                                                <td>
                                                    <span class="text-dark-75 font-weight-bolder d-block font-size-lg">Pasif</span>
                                                </td>
                                            }
                                            <td>
                                                <span class="text-dark-75 font-weight-bolder d-block font-size-lg">@item.SEOLink</span>
                                            </td>
                                            <td class="pr-0 text-right">
                                                <a href="~/Admin/Menu/Detay/@(item.Id)" class="btn btn-light-success font-weight-bolder font-size-sm">
                                                    <i class="fa fa-edit"></i>
                                                </a>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                        <!--end::Table-->
                    </div>
                </div>
